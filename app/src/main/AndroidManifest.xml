<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.iiotcorner">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ellogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ellogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Analogicas"></activity>
        <activity android:name=".RealTime" />
        <activity android:name=".Estadistico" />
        <activity android:name=".Configurar" />
        <activity android:name=".MenuPpal" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>